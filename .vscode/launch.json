{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "flashcoders",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "5000",
        "--web-renderer",
        "html"
      ]
    },
    {
      "name": "flashcoders (Edge)",
      "request": "launch",
      "type": "dart",
      "args": [
        "-d",
        "edge",
        "--web-port",
        "5000",
        "--web-renderer",
        "html"
      ]
    },
    {
      "name": "flashcoders (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flashcoders (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}